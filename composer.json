{
  "name": "boardlead/laravel-survey",
  "description": "Create surveys inside your Laravel app",
  "keywords": [
    "laravel",
    "survey"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "CSP",
      "email": "csp@csp.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/database": "^10.0|^11.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0|^11.0",
    "orchestra/testbench": "^8.0|^9.0",
    "illuminate/http": "^10.0|^11.0",
    "laravel/legacy-factories": "^1.4"
  },

  "config": {
      "optimize-autoloader": true,
      "preferred-install": "dist",
      "sort-packages": true
  },
  
  "autoload": {
    "psr-4": {
      "BLSurvey\\": "src/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },

  "autoload-dev": {
    "psr-4": {
      "BLSurvey\\Tests\\": "tests/"
    },
    "files": [
      "tests/utilities/helpers.php"
    ]
  },

  "extra": {
    "laravel": {
      "providers": [
        "BLForm\\Providers\\SurveyServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump"
        ]
    }
}
